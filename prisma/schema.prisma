generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}



model Purchase {
  id        String   @id @default(uuid())
  firstname String
  lastname  String
  email     String   
  phone     String
  product   String
  productname String
  price     Float
  country   Countrys
}

enum Countrys {
  United_States
  Canada
  United_Kingdom
  Australia
  Philippines
  Other
}